{
  "name": "snipo",
  "displayName": "snipo",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snipo.start",
        "title": "Snipo"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./src/snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./src/snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./src/snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./src/snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}